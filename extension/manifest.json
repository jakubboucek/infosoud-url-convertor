{
  "manifest_version": 3,
  "name": "InfoSoud URL Fixer",
  "version": "1.0.2",
  "description": "Automaticky přesměruje staré InfoSoud URL (justice.cz) na nové (gov.cz)",
  "homepage_url": "https://github.com/jakubboucek/infosoud-url-convertor",
  "permissions": ["declarativeNetRequest"],
  "host_permissions": ["https://infosoud.justice.cz/*", "https://infosoud-fixer.web.app/*"],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules-prod.json"
    },{
      "id": "ruleset_2",
      "enabled": false,
      "path": "rules-dev.json"
    }]
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
